{
  "name": "antwar-reason",
  "namespace": false,
  "reason": { "react-jsx": 2 },
  "refmt": 3,
  "bs-dependencies": ["reason-react"],
  "sources": [
    { "dir": "src", "subdirs": true },
    {
      "dir": "test",
      "subdirs": [
        "common",
        "pages",
        "components",
        "bindings",
        "utils",
        "layouts"
      ]
    }
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bsc-flags": ["-bs-no-version-header", "-bs-super-errors", "-bs-g"],
  "gentypeconfig": {
    "language": "untyped",
    "shims": []
  }
}
